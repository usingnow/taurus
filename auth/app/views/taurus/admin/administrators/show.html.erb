<div class="well">
  <%= link_to "<i class='icon-edit'></i> #{t('admin.actions.edit.menu')}".html_safe, edit_admin_administrator_path(@administrator), :class => "btn btn-primary" %>
  <%= link_to "<i class='icon-arrow-left'></i> #{t('admin.misc.back')}".html_safe, admin_administrators_path, :class => "btn pull-right" %>
</div>

<%= flash_messages %>

<%= simple_form_for(@administrator, :url => "#", :html => {:class => 'form-horizontal'}) do |f| %>
  <fieldset>
    <%= f.input :email, :disabled => true %>
    <%= f.input :name, :disabled => true %>
    <%= f.input :address, :disabled => true %>
    <%= f.input :phone, :disabled => true %>
    <%= f.input :fax, :disabled => true %>
    <%= f.input :mobile, :disabled => true %>
    <%= f.input :zip_code, :disabled => true %>
    <%= f.input :status, :as => :boolean, :disabled => true %>
    <%= f.association :groups, :disabled => true %>
  </fieldset>
<% end %>





