<h1>Listing product_purchaseships</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Purchase</th>
    <th>Quantity</th>
    <th>Unit price aft tax</th>
    <th>Total amount</th>
    <th>Delivery date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_purchaseships.each do |product_purchaseship| %>
  <tr>
    <td><%= product_purchaseship.product_id %></td>
    <td><%= product_purchaseship.purchase_id %></td>
    <td><%= product_purchaseship.quantity %></td>
    <td><%= product_purchaseship.unit_price_aft_tax %></td>
    <td><%= product_purchaseship.total_amount %></td>
    <td><%= product_purchaseship.delivery_date %></td>
    <td><%= link_to 'Show', admin_product_purchaseship_path(product_purchaseship) %></td>
    <td><%= link_to 'Edit', edit_admin_product_purchaseship_path(product_purchaseship) %></td>
    <td><%= link_to 'Destroy', admin_product_purchaseship_path(product_purchaseship), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

