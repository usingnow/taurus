<%= simple_form_for([:commodity, @sku_display]) do |f| %>
  <%= f.error_notification %>

  <div class="attributes">
    <%= f.input :sku_number %>
    <%= f.input :sequence %>
    <%= f.input :page do
      f.collection_select :page, @sku_display.page_enum , :first, :second, :checked => 0
    end %>
    <%= f.input :location do
      f.collection_select :location, @sku_display.location_enum(0) , :first, :second, :checked => 0
    end%>
  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
