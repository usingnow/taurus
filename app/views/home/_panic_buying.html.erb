<div id="panic_buying" class="smallslider">
  <ul>
    <% @panic_buyings.each do |buy| %>
      <li><a href="<%= buy.href %>" target="_blank"><img src="<%= buy.image.url %>" title="" alt="<%= buy.name %>" /></a></li>
    <% end %>
  </ul>
</div>

<% content_for :ready do %>
  jQuery('#panic_buying').smallslider({
   showText:false
  });
<% end %>
