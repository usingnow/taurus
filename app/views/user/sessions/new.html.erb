<h2>壹美壹家</h2>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <div><%= f.label :login_no %><br />
  <%= f.text_field :login_no %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.submit "登录" %></div>
<% end %>