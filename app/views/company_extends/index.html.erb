<% @company_extends.each do |company_extend| %>
  <tr>
    <td><%= company_extend.user_id %></td>
    <td><%= company_extend.company_no %></td>
    <td><%= company_extend.role_no %></td>
    <td><%= company_extend.industry_type_id %></td>
    <td><%= company_extend.company_type_id %></td>
    <td><%= company_extend.district_no %></td>
    <td><%= company_extend.company_scale_id %></td>
    <td><%= company_extend.email %></td>
    <td><%= company_extend.company_name %></td>
    <td><%= company_extend.contact_dept %></td>
    <td><%= company_extend.contact_post %></td>
    <td><%= company_extend.web_address %></td>
    <td><%= company_extend.sex %></td>
    <td><%= company_extend.name %></td>
    <td><%= company_extend.address %></td>
    <td><%= company_extend.phone %></td>
    <td><%= company_extend.fax %></td>
    <td><%= company_extend.mobile %></td>
    <td><%= company_extend.zip %></td>
    <td><%= company_extend.created_by %></td>
    <td><%= company_extend.updated_by %></td>
    <td><%= link_to 'Show', company_extend %></td>
    <td><%= link_to 'Edit', edit_company_extend_path(company_extend) %></td>
    <td><%= link_to 'Destroy', company_extend, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

