<%= form_for([:admin,@sku_productship]) do |f| %>
  <% if @sku_productship.errors.any? %>
    <div id="error_explanation">
      <h2><%= t('activerecord.errors.template.header', :count => @sku_productship.errors.size, :model => t('models.sku_productship')) %></h2>

      <ul>
      <% @sku_productship.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :product_id %><span class="red">*</span><br />
    <%= f.text_field :product_id %>
  </div>
  <div class="field">
    <%= f.label :package_name %><br />
    <%= f.text_field :package_name %>
  </div>
  <div class="field">
    <%= f.label :package_num %><br />
    <%= f.text_field :package_num %>
  </div>
  <div class="field">
    <%= f.label :sales_bef_tax %><br />
    <%= f.text_field :sales_bef_tax %>
  </div>
  <div class="field">
    <%= f.label :sales_aft_tax %><br />
    <%= f.text_field :sales_aft_tax %>
  </div>
  <div class="field">
    <%= f.label :is_main %><br />
    <%= f.check_box :is_main %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
