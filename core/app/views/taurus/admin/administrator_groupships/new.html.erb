<div class="form-actions">
  <%= link_to "<i class='icon-arrow-left'></i> 返回".html_safe, admin_administrators_path, :class => "btn pull-right" %>
</div>

<%= simple_form_for([:admin,@administrator_groupship], :html => {:class => "form-horizontal"}) do |f| %>
  <%= f.error_notification %>
  <fieldset>

    <%= hidden_field_tag 'administrator_id', @administrator_groupship.administrator_id %>
   <div class="control-group">
     <label id="trk-admin-group-setting-name" class="control-label alert alert-info" for="group_id[]">显示当前用户名</label>
      <div class="controls">
        <%=raw administrator_group(@administrator_groupships) %>
      </div>
   </div>
    <div class="form-actions">
      <%= f.button :submit, :value => "保存", :class => "btn btn-primary" %>
    </div>
  </fieldset>
<% end %>
